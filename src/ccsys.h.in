typedef long int			ccsys_time_unit_t;
typedef struct ccsys_seconds_t		ccsys_seconds_t;
typedef struct ccsys_milliseconds_t	ccsys_milliseconds_t;
typedef struct ccsys_microseconds_t	ccsys_microseconds_t;
