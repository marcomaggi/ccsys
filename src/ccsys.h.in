ccsys_in_parent_after_fork (ccsys_pid_t pid)
{
  return (pid.data)? true : false;
}

__attribute__((__always_inline__))
static inline bool
ccsys_in_child_after_fork (ccsys_pid_t pid)
{
  return (pid.data)? false : true;
}

